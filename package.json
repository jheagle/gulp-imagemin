{
	"name": "gulp-imagemin",
	"version": "9.1.0",
	"description": "Minify PNG, JPEG, GIF and SVG images",
	"license": "MIT",
	"repository": "sindresorhus/gulp-imagemin",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"main": "index.mjs",
	"exports": {
		".": {
			"import": {
				"default": "./index.mjs"
			},
			"require": {
				"default": "./cjs/index.js"
			}
		}
	},
	"sideEffects": false,
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"test": "xo && ava"
	},
	"files": [
		"index.mjs",
		"cjs"
	],
	"keywords": [
		"gulpplugin",
		"imagemin",
		"image",
		"img",
		"picture",
		"photo",
		"minify",
		"minifier",
		"compress",
		"png",
		"jpg",
		"jpeg",
		"gif",
		"svg"
	],
	"dependencies": {
		"chalk": "^5.3.0",
		"gulp-plugin-extras": "^1.0.0",
		"imagemin": "^9.0.0",
		"plur": "^5.1.0",
		"pretty-bytes": "^6.1.1"
	},
	"devDependencies": {
		"ava": "^6.1.3",
		"common-exports": "^1.3.2",
		"imagemin-pngquant": "^10.0.0",
		"vinyl": "^3.0.0",
		"xo": "^0.58.0"
	},
	"optionalDependencies": {
		"imagemin-gifsicle": "^7.0.0",
		"imagemin-mozjpeg": "^10.0.0",
		"imagemin-optipng": "^8.0.0",
		"imagemin-svgo": "^11.0.0"
	},
	"peerDependencies": {
		"gulp": ">=4"
	},
	"peerDependenciesMeta": {
		"gulp": {
			"optional": true
		}
	},
	"xo": {
		"ignores": [
			"cjs"
		],
		"rules": {
			"unicorn/prefer-module": 1
		}
	}
}
